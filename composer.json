{
    "name": "jithran/git-ribbon",
    "description": "A ribbon for laravel projects that gives information about the current git status of the project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jithran\\GitRibbon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jithran\\GitRibbon\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jithran",
            "email": "info@jithransikken.nl"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5"
    },
    "version": "1.0.4",
    "extra": {
        "laravel": {
            "providers": [
                "Jithran\\GitRibbon\\GitRibbonServiceProvider"
            ]
        }
    }
}
